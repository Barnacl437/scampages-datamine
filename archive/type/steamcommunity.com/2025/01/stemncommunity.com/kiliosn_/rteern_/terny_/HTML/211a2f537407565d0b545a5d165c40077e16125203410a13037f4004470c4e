              "BY",
              { bNoDefaultDescriptors: false }
            );
            GStoreItemData.SetCurrencyFormatter(function (
              nValueInCents,
              bWholeUnitsOnly
            ) {
              var fnBase = function (nValueInCents, bWholeUnitsOnly) {
                var fmt = function (nValueInCents, bWholeUnitsOnly) {
                  var format = v_numberformat(
                    nValueInCents / 100,
                    bWholeUnitsOnly ? 0 : 2,
                    ".",
                    ","
                  );
                  return format;
                };
                var strNegativeSymbol = "";
                if (nValueInCents < 0) {
                  strNegativeSymbol = "-";
                  nValueInCents = -nValueInCents;
                }
                return (
                  strNegativeSymbol + "$" + fmt(nValueInCents, bWholeUnitsOnly)
                );
              };
              return fnBase(nValueInCents, bWholeUnitsOnly) + " USD";
            });
            GStoreItemData.SetCurrencyMinPriceIncrement(1);
          </script>

