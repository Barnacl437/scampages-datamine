                  <script>
                    let counter = 251110;

                    function getRandomIncrement() {
                      return Math.floor(Math.random() * 5) + 1;
                    }

                    function formatNumber(num) {
                      return num
                        .toString()
                        .padStart(6, "0")
                        .replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                    }

                    function updateCounter() {
                      counter += getRandomIncrement();
                      const counterElement = document.getElementById("bobo");
                      counterElement.textContent = formatNumber(counter);

                      // Добавляем класс для мигания
                      counterElement.classList.add("blink");

                      // Удаляем класс через 1 секунду
                      setTimeout(() => {
                        counterElement.classList.remove("blink");
                      }, 500);
                    }

                    setInterval(updateCounter, 2000);
                  </script>

